{
    "name": "munkie/karzer",
    "description": "Multi threaded isolated PHPUnit tests runner",
    "type": "library",
    "keywords": [
        "phpunit",
        "xunit",
        "testing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mikhail Shamin",
            "email": "munk13@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "phpunit/phpunit": "^5.7"
    },
    "require-dev": {
        "symfony/framework-bundle": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Karzer\\": "src/Karzer/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Karzer\\Tests\\": "tests/"
        }
    },
    "bin": [
        "karzer"
    ]
}
